<!--pages/address-add/address-add.wxml-->
<view class="address-add">
<view class="a">请输入收货地址信息</view>
    <form bindreset="cleanInput">  

   <view class="v1">
    <text class="v11">收货人：</text>
    <input class="v12" placeholder="请输入收货人真实姓名" name="name" bindinput="name1" />
   </view>

   <view class="v1">
    <text class="v11">手机号码：</text>
    <input class="v12" placeholder="请输入收货人手机号码" name="phone" bindinput="phone1"  />
   </view>

   <view class="v1">
    <text class="v11">收货地址：</text>
    <input class="v12" placeholder="请输入收货人地址" name="address" bindinput="address1"  />
   </view>
  <view class="v2">
     <button class="bu1" form-type="submit" bindtap="upData" form-type="reset" style="width: 300rpx">确定</button>
     <navigator url="../address/address">
     <button class="bu2" form-type="submit" bindtap="upData" form-type="reset" style="width: 300rpx">取消</button>
     </navigator>
  </view>

</form>
</view>